<UserControl x:Class="DIV_Protos.InternalVersionRegistration"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:DIV_Protos" 
             mc:Ignorable="d">
    <UserControl.Resources>
        <SolidColorBrush x:Key="blueMahle" Color="#6d91aa" />
    </UserControl.Resources>
    <Grid Margin="20">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="Auto"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="Auto"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
            <ColumnDefinition Width="*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="backButton" Grid.Row="0" Grid.Column="0" 
                Height="30" Width="70" VerticalAlignment="Top" Margin="0,0,30,30"
                Content="Back" FontWeight="Medium" Command="{Binding BackMenu}"
                BorderThickness="2" Background="{StaticResource blueMahle}" Foreground="White"/>
        <TextBlock x:Name="title" Grid.ColumnSpan="2" Grid.Row="0" Grid.Column="1"
                   Text="Alta definición de producto" FontSize="48" FontWeight="Light"
                   HorizontalAlignment="Center" Margin="0,0,0,40"/>
        <TextBlock x:Name="userText" Grid.ColumnSpan="3" Grid.Row="0" Grid.Column="3"
                   Text="Usuario: " FontSize="20" FontWeight="Light"
                   HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0, 0, 170, 0"/>
        <TextBlock x:Name="actualUser" Grid.Row="0" Grid.Column="4"
                   Text="{Binding ActualUser, UpdateSourceTrigger=PropertyChanged}" FontSize="20" FontWeight="Light"
                   HorizontalAlignment="Right" VerticalAlignment="Center" Margin="0,0,60,0"/>
        
        <TextBlock x:Name="VersionText" Grid.Row="1" Grid.Column="1" 
                   Text="Versión interna:" Margin="10"
                   HorizontalAlignment="Left" VerticalAlignment="Center" />
        <ComboBox x:Name="VersionTextBox" Grid.Row="1" Grid.Column="1" 
                 ItemsSource="{Binding ItemDescriptions, UpdateSourceTrigger=PropertyChanged}" 
                 SelectedItem="{Binding SelectedItemDescription, UpdateSourceTrigger=PropertyChanged}"
                 HorizontalAlignment="Left" Margin="100,0,0,0" Height="20" Width="120"/>

        <Button x:Name="btnOpenFile" Grid.Row="1" Grid.Column="2" 
                Content="Open file" Height="30" Width="80" Margin="20"
                HorizontalAlignment="Right" VerticalAlignment="Center"
                Click="btnOpenFile_Click"/>
        <TextBlock x:Name="textExcelName"  Grid.Row="1" Grid.Column="3"
                    HorizontalAlignment="Left" VerticalAlignment="Center" Margin="10"
                    MinWidth="200"
                    Text="{Binding ExcelName, UpdateSourceTrigger=PropertyChanged}"/>
        
        <StackPanel Grid.Row="2" Grid.Column="1" Grid.ColumnSpan="2" Margin="20"
                    MinHeight="300" MaxHeight="400"
                    VerticalAlignment="Stretch"> <!-- "{Binding Path=ActualHeight, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=UIElement}}"--> 
            <ScrollViewer Height="{Binding Path=ActualHeight, RelativeSource={RelativeSource Mode=FindAncestor, AncestorType=StackPanel}}"
                          MinHeight="300" MaxHeight="400" 
                Visibility="{Binding VisibleExcelContent, UpdateSourceTrigger=PropertyChanged}">
